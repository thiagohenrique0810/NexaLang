{
  "name": "nexalang",
  "displayName": "NexaLang",
  "description": "Syntax highlighting and official tooling for NexaLang (.nxl)",
  "version": "0.1.0",
  "publisher": "nexalang",
  "icon": "nexalang-icon.svg",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nexalang",
        "aliases": ["NexaLang", "nexalang"],
        "extensions": [".nxl"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nexalang",
        "scopeName": "source.nexalang",
        "path": "./syntaxes/nexalang.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "nexalang",
        "path": "./snippets/nexalang.json"
      }
    ]
  }
}


